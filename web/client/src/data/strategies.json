{
  "indicators": [
    {
      "id": "six_veins",
      "name": "六脉神剑",
      "description": "多指标共振系统，由六个独立的技术指标组成，每个指标显示红色（多头）或绿色（空头）状态。",
      "subIndicators": [
        {
          "name": "MACD",
          "formula": "DIF > DEA",
          "meaning": "短期动能强于中期"
        },
        {
          "name": "KDJ",
          "formula": "K > D",
          "meaning": "超买超卖指标向上"
        },
        {
          "name": "RSI",
          "formula": "RSI5 > RSI13",
          "meaning": "短期相对强弱优于中期"
        },
        {
          "name": "LWR",
          "formula": "LWR1 > LWR2",
          "meaning": "威廉指标变种向上"
        },
        {
          "name": "BBI",
          "formula": "C > BBI",
          "meaning": "价格高于多空分界线"
        },
        {
          "name": "MTM",
          "formula": "MTM1 > MTM2",
          "meaning": "短期动量强于中期"
        }
      ],
      "signals": [
        {
          "type": "buy",
          "name": "涨买入",
          "condition": "六个指标首次全部变红",
          "action": "建仓或加仓"
        },
        {
          "type": "hold",
          "name": "上涨持有",
          "condition": "六个指标持续全红",
          "action": "持股待涨"
        },
        {
          "type": "sell",
          "name": "首次转绿",
          "condition": "六红变为非六红",
          "action": "减仓1/3"
        }
      ]
    },
    {
      "id": "buy_sell_points",
      "name": "买卖点指标",
      "description": "基于庄家线与散户线的交叉关系，结合吸筹指标识别主力动向。",
      "signals": [
        {
          "type": "buy",
          "name": "买点1",
          "condition": "吸筹值上穿14",
          "meaning": "主力开始吸筹",
          "position": "30%"
        },
        {
          "type": "buy",
          "name": "买点2",
          "condition": "庄家线上穿散户线且处于低位",
          "meaning": "主力拉升核心信号",
          "position": "50%"
        },
        {
          "type": "sell",
          "name": "卖点1",
          "condition": "庄家线下穿88",
          "meaning": "高位见顶",
          "action": "减仓50%"
        },
        {
          "type": "sell",
          "name": "卖点2",
          "condition": "散户线上穿庄家线",
          "meaning": "趋势反转",
          "action": "清仓"
        }
      ]
    },
    {
      "id": "money_tree",
      "name": "黄金摇钱树",
      "description": "技术形态选股指标，结合顶底识别、动量交叉和KDJ变种三重过滤。",
      "signals": [
        {
          "type": "buy",
          "name": "摇钱树选股",
          "condition": "XG66 AND XG55 AND XG88",
          "action": "强烈买入"
        }
      ]
    },
    {
      "id": "chan_lun",
      "name": "缠论买卖点 (5买3卖)",
      "description": "基于笔结构识别，严格按照通达信缠论公式实现，包含5个买点和3个卖点。买点分为一买、二买、三买、强二买、类二买。",
      "subIndicators": [
        {
          "name": "笔结构",
          "formula": "顶底分型识别",
          "meaning": "识别上涨笔和下跌笔"
        },
        {
          "name": "高低点序列",
          "formula": "GG/DD序列计算",
          "meaning": "5日周期的高低点统计"
        },
        {
          "name": "EMA均线",
          "formula": "EMA13/EMA26",
          "meaning": "趋势方向判断"
        }
      ],
      "signals": [
        {
          "type": "buy",
          "name": "一买 (chan_buy1)",
          "condition": "五段下跌 + 底背驰 + 方向向上",
          "formula": "方向=1 AND L<MA13 AND 五段下跌结构",
          "risk": "高（抄底）",
          "position": "20%",
          "meaning": "趋势反转的第一个买点，风险最高但收益最大"
        },
        {
          "type": "buy",
          "name": "二买 (chan_buy2)",
          "condition": "三段/五段下跌后回踩不破前低",
          "formula": "DD1<GG1 AND DD1>DD2 AND 三段上涨结构",
          "risk": "中",
          "position": "40%",
          "meaning": "确认上涨趋势的买点，性价比最高"
        },
        {
          "type": "buy",
          "name": "三买 (chan_buy3)",
          "condition": "回踩不进入中枢",
          "formula": "DD1>MIN(GG2,GG3) AND GG3>DD2",
          "risk": "低（追涨）",
          "position": "30%",
          "meaning": "趋势延续的买点，风险最低但空间有限"
        },
        {
          "type": "buy",
          "name": "强二买 (chan_strong_buy2)",
          "condition": "特殊结构的二买变种",
          "formula": "方向=1 AND C<MA13 AND DD3<DD2 AND DD3<DD1",
          "risk": "中",
          "position": "35%",
          "meaning": "强势回踩后的买点，背驰结构更明显"
        },
        {
          "type": "buy",
          "name": "类二买 (chan_like_buy2)",
          "condition": "与强二买逻辑相同",
          "formula": "同强二买",
          "risk": "中",
          "position": "35%",
          "meaning": "类似二买的变种结构"
        },
        {
          "type": "sell",
          "name": "一卖 (chan_sell1)",
          "condition": "五段上涨 + 顶背驰",
          "formula": "方向=-1 AND H>MA13 AND 五段上涨结构",
          "action": "清仓",
          "meaning": "趋势反转的第一个卖点"
        },
        {
          "type": "sell",
          "name": "二卖 (chan_sell2)",
          "condition": "回抽确认卖点",
          "formula": "GG1>DD1 AND GG1<GG2 AND 三段下跌结构",
          "action": "减仓50%",
          "meaning": "确认下跌趋势的卖点"
        },
        {
          "type": "sell",
          "name": "三卖 (chan_sell3)",
          "condition": "中枢跌破卖点",
          "formula": "GG1<MAX(DD2,DD3)",
          "action": "清仓",
          "meaning": "趋势延续下跌的卖点"
        }
      ]
    }
  ],
